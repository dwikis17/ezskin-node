{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "no-unused-vars": 0,
        "import/prefer-default-export": "off",
        "max-len": "off",
        "import/extensions": ["never"],
        "comma-dangle": "off",
        "no-underscore-dangle": ["error", {"allow" :  ["_id"]}],
        "arrow-body-style": "off",
        "no-restricted-syntax" : "off",
        "linebreak-style" :"off"
    }
}
